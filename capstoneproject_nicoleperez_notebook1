  {
   "cell_type": "code",
   "execution_count": 23,
   "id": "2c2d55f8-c372-4e2c-b472-a18ca8fd158a",
   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "\n",
    "# Define data types for loading to optimise memory\n",
    "order_products_dtypes = {\n",
    "    'order_id': 'int32',\n",
    "    'product_id': 'int32',\n",
    "    'add_to_cart_order': 'int16',\n",
    "    'reordered': 'int8',\n",
    "}\n",
    "\n",
    "# Load datasets with a limit on the number of rows to fit memory constraints\n",
    "orders = pd.read_csv('orders.csv', nrows=50000)  # Adjust as necessary\n",
    "products = pd.read_csv('products.csv', nrows=10000)  # Adjust as necessary\n",
    "order_products_prior = pd.read_csv('order_products__prior.csv', dtype=order_products_dtypes, nrows=500000)\n",
    "\n",
    "# Merge datasets\n",
    "merged_data = orders.merge(order_products_prior, on='order_id', how='inner').merge(products, on='product_id', how='inner')\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "id": "6e636065-45a9-480c-9d4d-70a8a1da0f5c",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Creating user-product matrix for clustering\n",
    "user_product_matrix = merged_data.groupby(['user_id', 'product_id'])['reordered'].count().unstack(fill_value=0)\n",
    "\n",
    "# Normalize the matrix to prepare for clustering\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "\n",
    "scaler = StandardScaler()\n",
    "normalized_matrix = scaler.fit_transform(user_product_matrix)\n",
    "\n",
    "# Convert to DataFrame for further processing\n",
    "norm_df = pd.DataFrame(normalized_matrix, index=user_product_matrix.index, columns=user_product_matrix.columns)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "b3c34fbc-14ac-4f49-9599-dfca0a98645c",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.cluster import KMeans\n",
    "\n",
    "# Set the number of clusters\n",
    "k = 10  # You can adjust this number based on your analysis\n",
    "kmeans = KMeans(n_clusters=k, random_state=42)\n",
    "clusters = kmeans.fit_predict(norm_df.T)  # Transpose for products\n",
    "\n",
    "# Assign cluster labels to products\n",
    "clustered_products = pd.DataFrame({'product_id': norm_df.columns, 'cluster': clusters})\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "id": "11c4141f-1e3c-455f-83d2-b0c1cb31a5d4",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Merge the clustered information back into the original dataset\n",
    "merged_data = merged_data.merge(clustered_products, on='product_id', how='left')\n",
    "\n",
    "# Prepare features and target variable\n",
    "X = merged_data[['order_id', 'cluster']]  # Sample features to predict based on old data\n",
    "y = merged_data['product_id']  # Target variable\n",
    "\n",
    "# One-hot encode cluster for Decision Tree input\n",
    "X = pd.get_dummies(X, columns=['cluster'], drop_first=True)"
   ]
  },
